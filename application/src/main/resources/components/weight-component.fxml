<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>


<HBox alignment="CENTER" fillHeight="false" prefHeight="36.0" prefWidth="856.0" spacing="40.0" styleClass="progres" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="timestip" alignment="CENTER" prefHeight="127.0" prefWidth="264.0" text="Tijdstip 1" HBox.hgrow="NEVER">
         <styleClass>
            <String fx:value="tijdstip" />
            <String fx:value="theme" />
         </styleClass>
      </Label>
      <ProgressBar fx:id="bar" prefHeight="36.0" prefWidth="820.0" progress="1.0" styleClass="progres-bar" />
      <Label fx:id="procentLabel" alignment="CENTER" prefHeight="32.0" prefWidth="0.0" text="100 %" HBox.hgrow="NEVER">
         <styleClass>
            <String fx:value="span-text" />
            <String fx:value="medium" />
         </styleClass>
      </Label>
   </children>
</HBox>
